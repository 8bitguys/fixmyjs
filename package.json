{
  "name": "fixmyjs",
  "version": "0.12.0",
  "description": "Automatically fixes your JavaScript based on lint rules",
  "homepage": "http://fixmyjs.com",
  "author": "Josh Perez <josh@goatslacker.com> (http://github.com/goatslacker)",
  "bin": {
    "fixmyjs": "./bin/fixmyjs"
  },
  "keywords": [
    "beautify",
    "jshint",
    "hint",
    "lint",
    "jslint"
  ],
  "main": "./lib/index.js",
  "license": {
    "type": "MIT",
    "url": "http://josh.mit-license.org"
  },
  "dependencies": {
    "esprima": "*",
    "escodegen": "*",
    "fu": "*",
    "commander": "*",
    "diff": "*",
    "minimatch": "*",
    "jshint": "*"
  },
  "devDependencies": {
    "testla": "*"
  },
  "repository": "git://github.com/jshint/fixmyjs",
  "preferGlobal": true,
  "scripts": {
    "test": "./node_modules/testla/bin/testla test"
  },
  "engines": {
    "node": "*"
  }
}
